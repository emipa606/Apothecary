            </RecipeDef>
          </value>
        </li>
        <!-- Sunflower -->
        <li Class="PatchOperationAdd">
          <xpath>/Defs</xpath>
          <value>
            <RecipeDef>
              <defName>Make_AYFRMT_SunflowerOil</defName>
              <label>make sunflower oil</label>
