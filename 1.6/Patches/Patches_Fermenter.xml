              <thingDefs>
                <li>AYGarlicBulb</li>
                <li>AYGingerRoot</li>
                <li>AYGinkgoSeeds</li>
                <li>AYGinsengRoot</li>
                <li>AYKaleLeaves</li>
                <li>AYTurmericRoot</li>
              </thingDefs>
            </value>
