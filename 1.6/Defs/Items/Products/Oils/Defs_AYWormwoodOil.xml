    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>7.2</MarketValue>
      <Mass>0.1</Mass>
      <Flammability>0.70</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
